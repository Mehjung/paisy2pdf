{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "paisy2pdf",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "dialog": {
        "all": true
      },
      "shell": {
        "all": false,
        "open": true
      },
      "path": {
        "all": true
      }
    },
    "windows": [
      {
        "title": "PAISYS PDF Converter",
        "width": 800,
        "height": 600,
        "fileDropEnabled": false
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.paisy2pdf",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://mehjung.github.io/paisy2pdf/releases/download/{{current_version}}/update.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDYxMTI5RDE2NjA3Q0Y3MTkKUldRWjkzeGdGcDBTWVJhS21TYXFwS0VlMTlZUERTT05xTWVKK3pXZ2FrK2pNK3dHd2lHWG9WVXQK",
      "windows": {
        "installMode": "passive"
      }
    }
  }
}
